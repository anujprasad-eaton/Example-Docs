{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "docker*.yml",
          "Docker*"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.pptx"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],              
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Eaton Docs",
      "_appFooter": "Eaton IT Innovation Documentation &copy; 2021",
      "_appLogoPath": "media/Eaton_PBW_Lit_RGB-160by64.png",
      "_disableContribution": false,
      "_appFaviconPath": "media/favicon.png",
      "breadcrumb_path": "~/Dynamics365/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "titleSuffix": "Dynamics 365",
      "_enableSearch": "true",            
      "recommendations": "true",
      "hideScope": false
    }, 
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/eaton"
    ],
    "postProcessors": ["ExtractSearchIndex"],    
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}